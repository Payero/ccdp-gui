{
  "name" : "CSV Test Thread",
  "cpu": 100,
  "description" : "CSV Reading and filtering demo",
  "tasks" : [ {
    "name" : "csv-reader",
    "description" : "Opens a single csv file and process line by line",
    "retries" : 3,
    "command" : [ "/data/ccdp/ccdp-engine/python/ccdp_mod_test.py", "-a", "testRandomTime", "-p", "min=20,max=25" ],
    "configuration" : { "filename": "/data/ccdp/webapp/frontend/server/data/it_help_desk.csv", 
                        "send-header": "True", 
                        "number-entries": 1},
    "task-id" : "csv-reader",
    "session-id" : "DEFAULT",
    "node-type" : "DEFAULT",
    "state": "PENDING",
    "reply-to" : "CCDP-WebServer",
    "input-ports" : [ ],
    "output-ports" : [ {
      "port-id" : "csv-reader",
      "from-port" : [ ],
      "to-port" : [ "csv-selector" ]
    } ]
  },
  {
    "name" : "csv-selector",
    "cpu": 100,
    "description" : "Selects all entries matching a simple criteria",
    "retries" : 3,
    "command" : [ "/data/ccdp/ccdp-engine/python/ccdp_mod_test.py", "-a", "testRandomTime", "-p", "min=35,max=40" ],
    "configuration" : { "field": "daysOpen", 
                        "operator": "GE", 
                        "value": 5},
    "task-id" : "csv-selector",
    "session-id" : "DEFAULT",
    "node-type" : "DEFAULT",
    "state": "PENDING",
    "reply-to" : "CCDP-WebServer",
    "input-ports" : [ ],
    "output-ports" : [ {
      "port-id" : "csv-selector",
      "from-port" : [ ],
      "to-port" : [ "csv-display" ]
    } ]
  },
  {
    "name" : "csv-display",
    "cpu": 100,
    "description" : "Displays filtered entries",
    "retries" : 3,
    "command" : [ "/data/ccdp/ccdp-engine/python/ccdp_mod_test.py", "-a", "testRandomTime", "-p", "min=45,max=50" ],
    "configuration" : { "output-url": "file:///tmp/results.csv"},
    "task-id" : "csv-display",
    "session-id" : "DEFAULT",
    "node-type" : "DEFAULT",
    "state": "PENDING",
    "reply-to" : "CCDP-WebServer",
    "input-ports" : [ ],
    "output-ports" : [ ]
  } ],  
  "thread-id" : "csv-reader-thread-1",
